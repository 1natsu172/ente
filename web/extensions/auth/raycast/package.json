{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ente-auth-raycast",
  "version": "0.0.0",
  "private": true,
  "title": "Ente Auth raycast extention",
  "description": "Quick access to otp items in ente auth.",
  "icon": "command-icon.png",
  "categories": [
    "Security",
    "Applications",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Typeahead Results",
      "description": "Template for showing typeahead search with network results.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "@raycast/utils": "^1.4.8",
    "@ente/accounts": "*",
    "@ente/shared": "*"
  },
  "devDependencies": {
    "@ente/eslint-config": "*",
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "ray": "ray",
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
